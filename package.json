    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.1",
    "react-icons": "^4.10.1",
    "react-redux": "^8.0.1",
    "redux-injectors": "^1.3.0",
    "redux-saga": "^1.1.3",
