{
  "name": "celo-edu",
  "author": "Muhindo Galien",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "prettier": "prettier --check \"src/**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
    "checkTs": "tsc",
    "lint:es": "next lint",
    "lint": "yarn prettier && yarn lint:es && yarn checkTs",
    "postinstall": "husky install",
    "prepare": "husky install",
    "contracts:compile:abi": "typechain --target web3-v1 --out-dir src/abis/types \"./src/abis/**/*.json\""
  },
