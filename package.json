    "@mui/material": "^5.7.0",
    "@mui/x-data-grid": "^5.11.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "clsx": "^1.1.1",
